<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Venta.
    -->
    <changeSet id="20210127141922-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="butaca_id"
                                 baseTableName="venta"
                                 constraintName="fk_venta_butaca_id"
                                 referencedColumnNames="id"
                                 referencedTableName="butaca"/>

        <addForeignKeyConstraint baseColumnNames="proyeccion_id"
                                 baseTableName="venta"
                                 constraintName="fk_venta_proyeccion_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proyeccion"/>

        <addForeignKeyConstraint baseColumnNames="pelicula_id"
                                 baseTableName="venta"
                                 constraintName="fk_venta_pelicula_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pelicula"/>

    </changeSet>
</databaseChangeLog>
